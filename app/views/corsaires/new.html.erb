<h1>Create New User</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@corsaire) do |f| %>
      <%= f.label :first_name %>
      <%= f.text_field :first_name, placeholder: "Juan Pablo" %>

      <%= f.label :age %>
      <%= f.text_field :age, placeholder: "34" %>

      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>


